#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;

int main(int argc, char** argv)
{
    int n;
    cin>>n;
    int i=0;
    int digits[10]={};
    while(n>0){
        digits[i]=n%10;
        n/=10;
        i++;
    }
    for(int j=0;j<i/2;j++){
        swap(digits[j],digits[i-j-1]); // coz numbers get added in reverse
    }
    next_permutation(digits,digits+i);
    for(int j=0;j<i;j++){
        cout<<digits[j];
    }
    return 1;
}
