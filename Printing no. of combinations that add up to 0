#include <iostream>
#include <cstdio>
#include <vector>
#include <algorithm>

int main() {
    std::vector<int>nums(1000);
    int size=0;
    char temp=0;
    while(true){
        scanf("%d%c",&nums[size++],&temp);
        if(temp=='\n') break;
    }
    std::sort(nums.begin(),nums.begin()+size);
    bool yes=false;
    for(int x=0;x<size;x++) std::cout<<nums[x]<<" ";
    std::cout<<std::endl;
    for(int i=0;i<size-2;i++){  // first I had done j=i+1 and k=j+1 but that repeated some combinations
        if(nums[i]==nums[i+1]) continue; // to skip redundant cases
        for(int j=i;j<size-1;j++){
            if(nums[i]==nums[j] && nums[i]==0) continue; // to skip 0 0 0 case
            for(int k=j;k<size;k++){
                if(nums[i]+nums[j]+nums[k]==0){
                    std::cout<<nums[i]<<' '<<nums[j]<<' '<<nums[k]<<std::endl;
                    yes=true;
                }
            }   
        }
    }
    if(yes==false) std::cout<<"None";
    return 7;
}
