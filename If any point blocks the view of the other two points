#include <iostream>
#include <cmath>
using namespace std;

int main() {
	int x1=0,y1=0,x2=0,y2=0,x3=0,y3=0,x4=0,y4=0;
	cin>>x1>>y1; //A
	cin>>x2>>y2; //B
	cin>>x3>>y3; //C
	cin>>x4>>y4; //D
	bool cross = false;
	
	if((sqrt(pow((x1-x3),2)+pow((y1-y3),2)) + sqrt(pow((x3-x2),2)+pow((y3-y2),2))) == sqrt(pow(x1-x2,2)+pow(y1-y2,2))) cross=true;
	
	if((sqrt(pow((x1-x4),2)+pow((y1-y4),2)) + sqrt(pow((x4-x2),2)+pow((y4-y2),2))) == sqrt(pow((x1-x2),2)+pow((y1-y2),2))) cross=true;
	
	if((sqrt(pow((x3-x1),2)+pow((y3-y1),2)) + sqrt(pow((x1-x4),2)+pow((y1-y4),2))) == sqrt(pow((x3-x4),2)+pow((y3-y4),2))) cross=true;
	
	if((sqrt(pow((x3-x2),2)+pow((y3-y2),2)) + sqrt(pow((x2-x4),2)+pow((y2-y4),2))) == sqrt(pow((x3-x4),2)+pow((y3-y4),2))) cross=true;
	
	
	(cross==true)?cout<<"yes":cout<<"no";
	return 0;
}
