#include <iostream>
using namespace std;
int main(){
	long n;
	cin>>n;
	int arr[n]={};
	long max=0,temp_max=0;
	long days=0,temp_days=0;
	for(int i=0;i<n;i++){
	    cin>>arr[i];    
	}
	for(long i = n-1 ; i>=0 ;){ 
	    temp_max=arr[i];
	    if(i==n-1) i--;
        if(i>=0){
            temp_days=0;
	        while(temp_max >= arr[i]){
	            temp_days++;
	            i--;
	        }
            if(temp_days>=days){
                max=temp_max;
                days=temp_days;
            }
        }
	}
	cout<<max;
	return 0;
}
