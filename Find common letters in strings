#include <iostream>

int main() {
	int n;
	std::cin>>n;
	std::string temp;
	int alpha[26]={};
	for(int i=0;i<n;i++){
	    std::cin>>temp;
	    for(int j=0;j<=temp.length();j++){
	        if(alpha[temp[j]-'a']==i){
	            alpha[temp[j]-'a']++;
	        }
	    }
	}
	bool flag=0;
	for(int i=0;i<26;i++){
	    if(alpha[i]==n){
	        std::cout<<(char)(i+'a');
	        flag=1;
	    }
	}
	if(flag==0){
        std::cout<<"-1";
	}
    return 7;
}
